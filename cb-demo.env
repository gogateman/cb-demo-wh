env:
  name: qa
  secret: 
    type: dockerconfigjson
    configjson: 
  cbservices:
    -
      cbservice:
        name: redis
        replicas: 1
        image: avarade/cb-redis
        port: 6379
    -
      cbservice:
        name: nginx
        type: LB
        replicas: 3
        image: avarade/cb-nginx
        port: 80
    -
      cbservice:
        name: node
        replicas: 3
        image: avarade/cb-node
        port: 8080
